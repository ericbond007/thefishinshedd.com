{
  "name": "thefishinshedd.com",
  "version": "2.0.0",
  "private": true,
  "homepage": "https://thefishinshedd.com",
  "dependencies": {
    "ajv": "^5.3.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "^6.26.0",
    "compression": "^1.7.1",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-animate-on-scroll": "^1.3.1",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.0.0",
    "react-ga": "^2.3.5",
    "react-helmet": "^5.2.0",
    "react-icons": "^2.2.7",
    "react-lazyload": "^2.3.0",
    "react-media": "^1.6.1",
    "react-overlays": "^0.8.3",
    "react-parallax-effect": "^1.0.6",
    "react-progressive-bg-image": "^2.1.2",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "react-snap": "^1.8.0",
    "webpack": "^3.8.1",
    "yarn": "^1.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "react-scripts": "^1.0.13",
    "eslint": "^4.9.0",
    "eslint-config-react-app": "^2.0.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0"
  },
  "reactSnap": {
    "skipThirdPartyRequests": true,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "dd": "yarn build && yarn deploy",
    "start": "react-scripts start",
    "go": "react-scripts build",
    "snap": "react-snap",
    "lint": "eslint .",
    "build": "react-scripts build && react-snap &&  zip -r shedd.zip build/*",
    "deploy": "aws s3 sync build/ s3://thefishinshedd.com --cache-control max-age=604800 --exclude \"img/*\" && aws s3 sync build/ s3://www.thefishinshedd.com --cache-control max-age=604800 --exclude \"img/*\"",
    "deployImg": "aws s3 sync build/ s3://thefishinshedd.com --cache-control max-age=604800 && aws s3 sync build/ s3://www.thefishinshedd.com --cache-control max-age=604800",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "babel": {
    "presets": [
      "env",
      "react-app"
    ]
  }
}
